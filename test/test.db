record(calc, "cnt$(S)") {
  field(INPA, "cnt$(S).VAL")
  field(INPB, "10")
  field(CALC, "C:=A>=B?0:(A<=0?1:C);C?A+1:A-1")
  field(SCAN, "1 second")
  field(LOW , "2")
  field(HIGH, "5")
  field(HIHI, "7")
  field(LSV , "INVALID")
  field(HSV , "MINOR")
  field(HHSV, "MAJOR")
}

record(bi, "static$(S)") {
  field(ZNAM, "0")
  field(ONAM, "1")
  field(OSV , "MAJOR")
  field(PINI, "YES")
}
